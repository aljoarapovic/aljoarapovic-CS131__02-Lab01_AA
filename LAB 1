public class Comparator{

	private Comparator() {
	  
	}
	
	public static int largestNumber(int x, int y) {
		 if(x>y)
        return x;
    else
        return y;
		
	}
	public static boolean isMultiple(int x, int y) {
		 if(x%y == 0)
        return true;
    else
        return false;
		
	}
	
	public static boolean bothDivisibleTen(int x, int y) {
		  if(x>=10 && y>=10)
    {
        if(x%10==0 && y%10==0)
            return true;
        else
            return false;
    }
    return false;
	}
	public static boolean isOddNumber(int x) {
	 if(x%2==0)
        return false;
    else
        return true;
	}
	
}




public class Cat {
   String breed;
   int age;
   String color;

   void meowing() {
   }

   void thirsty() {
   }

   void eating() {
   }
}


public class Kitty {
   int kittyAge;

   public class Kitty(String name) {
      System.out.println("Name chosen is :" + name );
   }

   public void setAge( int age ) {
      kittyAge = age;
   }

   public int getAge( ) {
      System.out.println("Kitty's age is :" + kittyAge );
      return kittyAge;
   }

   public static void main(String []args) {
      Kitty myKitty = new Kitty( "bella" );

      myKitty.setAge( 2 );

      myKitty.getAge( );

      System.out.println("Variable Value :" + myKitty.KittyAge );
   }
}





